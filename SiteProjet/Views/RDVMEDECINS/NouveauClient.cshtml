
@{
    ViewBag.Title = "NouveauClient";
}

<h2>Cabinet médical - LES MEDECINS ASSOCIES</h2>

<input type="button" value="Accueil" onclick="location.href='./Index'" />

@using (Html.BeginForm())
{
    <select name="titreClient">
        <option>Mr</option>
        <option>Mme</option>
        <option>Melle</option>
    </select>
    <label>Nom</label>
    <input type="text" name="nomClient" />
    <label>Prénom</label>
    <input type="text" name="prenomClient" />
    <input hidden name="jour" value="@ViewData["jour"]" />
    <input type="submit" value="Valider" />
}
